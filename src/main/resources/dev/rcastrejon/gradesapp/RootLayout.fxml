<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.rcastrejon.gradesapp.RootLayoutController">
   <top>
      <ButtonBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="generateButton" mnemonicParsing="false" onAction="#handleGenerate" text="Generar CSV" />
            <Button fx:id="loadButton" layoutX="535.0" layoutY="18.0" mnemonicParsing="false" onAction="#handleLoad" text="Cargar CSV" />
        </buttons>
         <BorderPane.margin>
            <Insets left="5.0" right="5.0" />
         </BorderPane.margin>
      </ButtonBar>
   </top>
</BorderPane>
